    if (!tube) return;
    if (gameWin) return;

    if (selectedBall) {
      const currentBall =3D selectedBall;
      if (canPlaceBall(tube, selectedBall)) {
        tube.prepend(currentBall);
        currentBall.classList.remove("selected");
        currentBall.classList.add("bounce");
        setTimeout(() =3D&gt; currentBall.classList.remove("bounce"), 1000)=
; // Retirer l'animation apr=C3=A8s 1s
        selectedBall =3D null;
        moveCount++;
        document.querySelector("#movements").innerText =3D moveCount;
        checkWinCondition();
      }
    } else {
      selectABall(tube);
    }
  });

  function selectABall(tube) {
    if (tube.children.length =3D=3D=3D 0) return;
    const topBall =3D tube.firstElementChild;
    selectedBall =3D topBall;
    topBall.classList.add("selected");
  }

  function canPlaceBall(tube, ball) {
    // V=C3=A9rifiez si le tube est plein
    if (tube.children.length &gt;=3D 4) return false;

    const topBall =3D tube.firstElementChild;
    if (!topBall) return true; // Si le tube est vide, la balle peut =C3=AA=
tre plac=C3=A9e

    if (topBall.style.backgroundColor =3D=3D=3D ball.style.backgroundColor)
      return true; // Si les couleurs correspondent, la balle peut =C3=AAtr=
e plac=C3=A9e

    return false; // Sinon, la balle ne peut pas =C3=AAtre plac=C3=A9e
  }

  function generateRandomLevel(colorCount) {
    const colors =3D [
      "red",
      "blue",
      "green",
      "yellow",
      "violet",
      "orange",
      "grey",
      "turquoise",
      "lightblue",
      "chartreuse",
    ];
    let balls =3D [];

    // cr=C3=A9ez une liste de balles bas=C3=A9e sur le nombre de couleurs =
s=C3=A9lectionn=C3=A9
    for (let i =3D 0; i &lt; colorCount; ++i) {
      for (let j =3D 0; j &lt; 4; ++j) {
        balls.push(colors[i]);
      }
    }

    // m=C3=A9langer les balles
    balls =3D shuffleArray(balls);

    // supprimer les tubes existants
    const existingTubes =3D document.querySelectorAll(".tube");
    existingTubes.forEach((tube) =3D&gt; tube.remove());

    // cr=C3=A9er les nouveaux tubes
    const tubeContainer =3D document.querySelector("main");
    for (let i =3D 0; i &lt; colorCount + 2; i++) {
      const tube =3D document.createElement("div");
      tube.classList.add("tube");
      tubeContainer.appendChild(tube);
    }

    //distribuer les balles
    const tubes =3D document.querySelectorAll(".tube");
    let ballIndex =3D 0;
    tubes.forEach((tube) =3D&gt; {
      for (let i =3D 0; i &lt; 4 &amp;&amp; ballIndex &lt; balls.length; ++=
i) {
        const ball =3D document.createElement("div");
        ball.setAttribute(
          "style",
          "background-color: " + balls[ballIndex] + ";"
        );
        //console.log(balls[ballIndex]);
        tube.appendChild(ball);
        ballIndex++;
      }
    });

    // r=C3=A9initialise le compteur de mouvements
    moveCount =3D 0;
    document.querySelector("#mouvements").innerText =3D moveCount;
  }

  function shuffleArray(array) {
    for (let i =3D array.length - 1; i &gt; 0; i--) {
      const j =3D Math.floor(Math.random() * (i + 1));
      [array[i], array[j]] =3D [array[j], array[i]];
    }
    return array;
  }

  function checkWinCondition() {
    if (gameOver) {
      setTimeout(() =3D&gt; {
        alert("Plus de coup disponible... Vous avez perdu =3D(");
      }, 600);
    }
    let win =3D true;
    document.querySelectorAll(".tube").forEach((tube) =3D&gt; {
      if (tube.children.length =3D=3D=3D 4) {
        // V=C3=A9rifiez si le tube est compl=C3=A8tement rempli
        const color =3D tube.children[0].style.backgroundColor;
        for (let ball of tube.children) {
          if (ball.style.backgroundColor !=3D=3D color) {
            win =3D false;
            break;
          }
        }
      } else if (tube.children.length &gt; 0) {
        // Si le tube n'est pas vide et n'est pas compl=C3=A8tement rempli,=
 le jeu n'est pas gagn=C3=A9
        win =3D false;
      }
    });
    if (win) {
      gameWin =3D true;
      setTimeout(() =3D&gt; {
        alert("Vous avez gagn=C3=A9!");
      }, 600);
    }
  }

  function isMoveAvailable() {
