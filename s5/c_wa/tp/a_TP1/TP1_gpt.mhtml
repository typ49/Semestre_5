    for (let tube of tubes) {
      if (tube.children.length =3D=3D=3D 0) {
        // Si un tube est vide, un coup est disponible
        return true;
      }

      const topBallColor =3D tube.firstElementChild.style.backgroundColor;

      for (let targetTube of tubes) {
        if (tube =3D=3D=3D targetTube) continue; // Ne v=C3=A9rifiez pas le=
 m=C3=AAme tube

        if (targetTube.children.length =3D=3D=3D 0) {
          // Si le tube cible est vide, un coup est disponible
          return true;
        }

        const targetTopBallColor =3D
          targetTube.firstElementChild.style.backgroundColor;

        if (topBallColor =3D=3D=3D targetTopBallColor) {
          // Si les couleurs des balles du dessus correspondent, un coup es=
t disponible
          gameOver =3D false;
        }
      }
    }

    // Si aucune des conditions ci-dessus n'est remplie, aucun coup n'est d=
isponible
    gameOver =3D true;
  }

  // Gestion du menu
