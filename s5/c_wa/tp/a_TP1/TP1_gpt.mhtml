) {
    if (
      confirm(
        "Voulez-vous vraiment g=C3=A9n=C3=A9rer un nouveau puzzle? Cela va =
r=C3=A9initialiser le score"
      )
    ) {
      const colorCount =3D parseInt(document.querySelector("#numColors").va=
lue);
      generateRandomLevel(colorCount);
